#include <windows.h>

// Icon with lowest ID value placed first to ensure it's used as application icon
IDI_ICON1 ICON "@CMAKE_SOURCE_DIR@/res/app.ico"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION 0,1,0,0
 PRODUCTVERSION 0,1,0,0
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Maxime Le Besnerais"
            VALUE "FileDescription", "Membrane - C++ and Web Tech Interface"
            VALUE "FileVersion", "0.1.0.0"
            VALUE "InternalName", "@BINARY_NAME@"
            VALUE "LegalCopyright", "Copyright (C) 2025 Maxime Le Besnerais. All rights reserved."
            VALUE "OriginalFilename", "@BINARY_NAME@.exe"
            VALUE "ProductName", "Membrane"
            VALUE "ProductVersion", "0.1.0.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END